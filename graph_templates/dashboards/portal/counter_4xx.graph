app = @properties[:app]
versions = @properties[:version]
envs = @properties[:environment]
title       "Respostas HTTP 4xx do BACKEND" 
vtitle      "respostas/s"
area        :none
description "Respostas 4xx para app #{app}"
linewidth 1
draw_null_as_zero true

envs.each do |env|
	versions.each do |version|
		params = {
			:data => "stats.timers.MireVeja.deliver.#{app}.#{env}.#{version}.MISS.4*.count_ps",
			:alias => "#{env} - #{version}"
		}
		params[:alias] = "#{env}" if versions.count <= 1
		field "#{env}#{version}", params
	end
end