<% unless @error %>
<div class="row-fluid">
  <%= erb :_dashboard_header %>
</div>
<div class="row-fluid">
  <div class="span2">
  </div>
  <div class="span8">
<%= erb :_interval_filter, :layout => false unless @interval_filters.empty? %>
  </div>
  <div class="span2">
  </div>
</div>
<table align=center>
<tr>
<% span=12/@graph_columns %>
<% @graphs.each_with_index do |graph,i| %>
  <% if graph %>
    <% if i>0 and i%@graph_columns==0 %>
      </tr>
<tr>
    <%end%>
<td>
        <%= erb(:graph, :locals => { :graph => graph }) %>
</td>
  <% end %>
<% end %>
</tr>
</table>
<% end %>
