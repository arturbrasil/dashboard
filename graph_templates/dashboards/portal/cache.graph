app = @properties[:app]
title       "Origem das respostas" 
vtitle      "responses/s"
area        :none
description "Requests respondidos com o cache ou backend para app #{app}"
linewidth 1
draw_null_as_zero true
["Cache", "Backend"].each do |cache|
	params = {
		:data => "avg(stats.timers.MireVeja.deliver.#{app}.*.*.*.*.#{cache}.*.count_ps)",
		:alias => cache
	}
	field "#{cache}", params
end
