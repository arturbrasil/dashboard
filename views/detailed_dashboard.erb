<% unless @error %>
<div class="row-fluid">
<%= erb :_dashboard_header %>
</div>
  
<% span=12/@graph_columns %>
<table align=center>
<tr>
<% @graphs.each_with_index do |graph,i| %>
  <% if graph %>
    <% if i>0 and i%@graph_columns==0 %>
       </tr>
       <tr>
    <%end%>
        <td>
          <%= erb(:graph, :locals => { :graph => graph, :omit_link => true }) %>
        </td>
  <% end %>
<% end %>
</tr>
</table>
<% end %>
