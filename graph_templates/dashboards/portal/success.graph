app = @properties[:app]
versions = @properties[:versions]
envs = @properties[:environments]
title       "HTTP 2xx" 
vtitle      "requests/s"
area        :none
description "Respostas 200 para app #{app}"
linewidth 1
draw_null_as_zero true
envs.each do |env|
	versions.each do |version|
		field "#{env}#{version}",
	  	:alias => "#{env} - #{version}",
	 	:data  => "stats.timers.MireVeja.deliver.#{app}.#{env}.#{version}.MISS.2*.count_ps"
	end
end