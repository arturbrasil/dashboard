app = @properties[:app]
title       "Origem das respostas" 
vtitle      "responses/s"
area        :none
description "Requests respondidos com o cache ou backend para app #{app}"
linewidth 1
draw_null_as_zero true
["MISS", "HIT"].each do |cache|
	label = {
		"MISS" => "backend",
		"HIT" => "cache"
	}
	params = {
		:data => "sum(stats.timers.MireVeja.deliver.#{app}.*.*.*.*.#{cache}.*.count_ps)",
		:alias => label[cache]
	}
	field "#{cache}", params
end
